{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "xo",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ]
    },
    "plugins": [
        "prettier"
    ]
}