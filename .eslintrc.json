{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "xo",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "warn",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}
